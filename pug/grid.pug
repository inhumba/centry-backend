						.grid 3/12
					.col-12.col-xl-6.col-md-6
						.grid 6/12
				//  End Grid
				|
				|
				// Grid : Desktop 2/12, 8/12, 2/12   , Tablet 2/12, 8/12, 2/12   , Mobile 12
				.row
					.col-12.col-xl-2.col-md-2
						.grid 2/12
					.col-12.col-xl-8.col-md-8
						.grid 8/12
					.col-12.col-xl-2.col-md-2
						.grid 2/12
				//  End Grid
				|
				|
				// Grid : Desktop 5/12, 4/12, 3/12   , Tablet 5/12, 4/12, 3/12   , Mobile 12
				.row
					.col-12.col-xl-5.col-md-5
						.grid 5/12
					.col-12.col-xl-4.col-md-4
						.grid 4/12
					.col-12.col-xl-3.col-md-3
						.grid 3/12
				// End Grid
				|
				|
			// ======================== End Page content =============================
			|
			|
			|
			|
			//-//- Include footer
			include master/footer.pug


	// Main Script
	block script
		include script/master-script.js


	// Custom Script
	block custom-script
		|
