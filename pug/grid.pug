						.grid 10/12
				//  End Grid
				|
				|
				// Grid : Desktop 3/12,9/12   , Tablet 3/12,9/12   , Mobile 12
				.row
					.col-12.col-xl-3.col-md-3
						.grid 3/12
					.col-12.col-xl-9.col-md-9
						.grid 9/12
				//  End Grid
				|
				|
				//  Grid : Desktop 4/12,8/12   , Tablet 4/12,8/12   , Mobile 12
				.row
					.col-12.col-xl-4.col-md-4
						.grid 4/12
					.col-12.col-xl-8.col-md-8
						.grid 8/12
				//  End Grid
				|
				|
				//  Grid : Desktop 5/12,7/12   , Tablet 5/12,7/12   , Mobile 12
				.row
					.col-12.col-xl-5.col-md-5
						.grid 5/12
					.col-12.col-xl-7.col-md-7
						.grid 7/12
				//  End Grid
				|
				|
				//  Grid : Desktop 6/12,6/12   , Tablet 6/12,6/12   , Mobile 12
				.row
					.col-12.col-xl-6.col-md-6
						.grid 6/12
					.col-12.col-xl-6.col-md-6
						.grid 6/12
				//  End Grid
				|
				|
				//  Grid : Desktop 7/12,5/12   , Tablet 7/12,5/12   , Mobile 12
				.row
					.col-12.col-xl-7.col-md-7
						.grid 7/12
					.col-12.col-xl-5.col-md-5
						.grid 5/12
				// End Grid
				|
				|
				// Grid : Desktop 8/12,4/12   , Tablet 8/12,4/12   , Mobile 12
				.row
					.col-12.col-xl-8.col-md-8
						.grid 8/12
					.col-12.col-xl-4.col-md-4
						.grid 4/12
				// End Grid
				|
				|
				//  Grid : Desktop 9/12,3/12   , Tablet 9/12,3/12   , Mobile 12
				.row
					.col-12.col-xl-9.col-md-9
						.grid 9/12
